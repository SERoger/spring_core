<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="empbean1" class="emp.Employee">
		<constructor-arg value="100" type="int" />
		<constructor-arg value="Preeti" />
		<constructor-arg value="30" type="int" />
		<constructor-arg>
			<ref bean="addrBean1" />
		</constructor-arg>
	</bean>
	<bean id="empbean2" class="emp.Employee">
		<constructor-arg value="101" type="int" />
		<constructor-arg value="Roger" />
		<constructor-arg value="29" type="int" />
		<constructor-arg>
			<ref bean="addrBean1" />
		</constructor-arg>
	</bean>

	<bean id="empbean3" class="emp.Employee">
		<constructor-arg value="100" type="int" />
		<constructor-arg value="Mike" />
		<constructor-arg value="28" type="int" />
		<constructor-arg>
			<ref bean="addrBean1" />
		</constructor-arg>
	</bean>

	<bean id="addrBean1" class="emp.Address">
		<constructor-arg value="1" type="int" />
		<constructor-arg value="741 Northern Avenue Apt#A3" />
		<constructor-arg value="Clarkston" />
		<constructor-arg value="GA" />
		<constructor-arg value="30021" type="int" />
		<constructor-arg value="Dekalb" />
		<constructor-arg value="Country" />
	</bean>
</beans>