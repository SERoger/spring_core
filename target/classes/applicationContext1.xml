<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Beans are singleton by default, so to create multiple objects from one bean you have to set scope="prototype" -->
	<!--Question -->
	<bean id="question" class="quiz.model.Question" scope="prototype">
	</bean>
	<!--Possible Answers -->
	<bean id="possibleAnswer" class="quiz.model.PossibleAnswer" scope="prototype">
	</bean>
	<!-- History -->
	<bean id="history" class="quiz.model.History" scope="prototype">
	</bean>
</beans>